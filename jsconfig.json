{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "jsx": "react",
    "strictNullChecks": true,
    "strictFunctionTypes": true
  },
  "exclude": [
    ".git",
    ".app-cache",
    ".npm",
    "build",
    "build*",
    "node_modules",
    "**/build/*",
    "**/node_modules/*"
  ]
}
